    "bowser": "^2.11.0",
    "inversify": "^5.1.1",
    "mustache": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.1.0",
    "swipe-listener": "^1.3.0",
    "typescript.events": "^7.9.0"
  }
